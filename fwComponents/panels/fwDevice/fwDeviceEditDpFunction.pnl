  <script name="ScopeLib" isEscaped="1"><![CDATA[dyn_dyn_anytype propertyDefinitionObject;

void populatePanel() {

  elementNameField.text = propertyDefinitionObject[1][1];
  text_function.text = propertyDefinitionObject[fwDpFunction_OBJ_FUNCTION][1];
  dyn_list_param.items = propertyDefinitionObject[fwDpFunction_OBJ_PARAM];
}

void saveSettings(dyn_string&amp;  exceptionInfo) {
  

 
    propertyDefinitionObject[fwDpFunction_OBJ_FUNCTION][1] = text_function.text;
    propertyDefinitionObject[fwDpFunction_OBJ_PARAM] =  dyn_list_param.items;
    fwGeneral_setGlobalValue($sDpeConfigObjectVarName, propertyDefinitionObject, exceptionInfo);
  		PanelOffReturn(makeDynFloat(0), makeDynString());
  
  
}]]></script>
  <script name="Initialize" isEscaped="1"><![CDATA[main()
{
  
  	dyn_string exceptionInfo;
  		if(isDollarDefined(&quot;$sDpeConfigObjectVarName&quot;))	// object passed to the panel
		{
			if(getGlobalType($sDpeConfigObjectVarName) != DYN_DYN_ANYTYPE_VAR)
			{
				nameText.text(&quot;Wrong object name&quot;);
				fwException_raise(exceptionInfo, 
													&quot;WARNING&quot;, &quot;The config object &quot; + $sDpeConfigObjectVarName + 
													&quot; doesn't exists or hasn't got the proper format.&quot;, 
													&quot;&quot;);
				fwExceptionHandling_display(exceptionInfo);					
				return;
			}													
			fwGeneral_getGlobalValue($sDpeConfigObjectVarName, propertyDefinitionObject, exceptionInfo);
   
   populatePanel();

 		}
    
}]]></script>
 </events>
