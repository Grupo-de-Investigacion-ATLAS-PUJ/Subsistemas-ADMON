    <script name="Initialize" isEscaped="1"><![CDATA[// SimpleCtrlScriptStart {invalid}
main()
{
  EP_setBackColor();
}

EP_setBackColor()
{
dyn_errClass err;

//DebugN($dpeName);
  if( $dpeName == &quot;&quot; )
  {
    setValue(&quot;&quot;, &quot;color&quot;, &quot;_dpdoesnotexist&quot;);
    return;
  }
  if( dpSubStr( $dpeName+&quot;:_online.._value&quot;, DPSUB_DP_EL ) == &quot;&quot; )
  {
    setValue(&quot;&quot;, &quot;color&quot;, &quot;_dpdoesnotexist&quot;);
    return;
  }
//DebugN($dpeName);
  dpConnect(&quot;EP_setBackColorCB&quot;, 
            $dpeName+&quot;:_online.._value&quot;);
  err = getLastError();
  if (dynlen(err) &gt; 0)
    setValue(&quot;&quot;, &quot;color&quot;, &quot;_dpdoesnotexist&quot;);
}


EP_setBackColorCB(string dpSource, int iNewValue)
{
//DebugN(dpSource,iNewValue);
	int configType;

	dpGet($sDpNameIssue+&quot;:_address.._type&quot;, configType);

	if(configType == DPCONFIG_NONE) {
	    setValue(&quot;&quot;, &quot;color&quot;, &quot;Blue&quot;);
	    return;
	}

  if (iNewValue == 0)
    setValue(&quot;&quot;, &quot;backCol&quot;, &quot;Darkgrey&quot;);
  else if (iNewValue == -1)
    setValue(&quot;&quot;, &quot;backCol&quot;, &quot;Red&quot;);
  else if (iNewValue == 1)
    setValue(&quot;&quot;, &quot;backCol&quot;, &quot;Green&quot;);
  else
    setValue(&quot;&quot;, &quot;backCol&quot;, &quot;Black&quot;);
}

// SimpleCtrlScript {EP_setBackColor}
// DP {$dpName}
// DPConfig {:_online.._value}
// DPType {int}
// AlertHdl {FALSE}
// DPEInvalid {}
// DPEConfigInvalid {}
// Operator1 {equal}
// Operant1 {0}
// Color1 {Darkgrey}
// Operator2 {equal}
// Operant2 {1}
// Color2 {Red}
// Operator3 {equal}
// Operant3 {2}
// Color3 {Green}
// Operator4 {else}
// Operant4 {}
// Color4 {Black}
// SimpleCtrlScriptEnd {EP_setBackColor}
]]></script>
