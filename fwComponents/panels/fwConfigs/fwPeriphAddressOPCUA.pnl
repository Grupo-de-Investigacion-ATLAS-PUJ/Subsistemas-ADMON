  <shape Name="Treiber" shapeType="SPIN_BUTTON" layerId="0">
   <properties>
    <prop name="serialId">14</prop>
    <prop name="Type"></prop>
    <prop name="RefPoint">558.8571428571429 33</prop>
    <prop name="Enable">True</prop>
