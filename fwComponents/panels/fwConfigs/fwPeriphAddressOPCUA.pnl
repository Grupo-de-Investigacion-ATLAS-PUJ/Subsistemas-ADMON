    <prop name="BackColor">_Window</prop>
    <prop name="TabOrder">94</prop>
    <prop name="ToolTipText">
     <prop name="en_US.utf8"></prop>
     <prop name="en_US.iso88591"></prop>
     <prop name="de_AT.iso88591"></prop>
    </prop>
    <prop name="layoutAlignment">AlignCenter</prop>
    <prop name="snapMode">Point</prop>
    <prop name="Font">
     <prop name="en_US.utf8">Arial,-1,13,5,50,0,0,0,0,0</prop>
     <prop name="en_US.iso88591">Arial,10,-1,5,50,0,0,0,0,0</prop>
     <prop name="de_AT.iso88591">Arial,10,-1,5,50,0,0,0,0,0</prop>
    </prop>
    <prop name="Location">561 35</prop>
    <prop name="Size">61 23</prop>
    <prop name="MinimumID">1</prop>
    <prop name="MaximumID">256</prop>
    <prop name="IncrAccelerator">1</prop>
    <prop name="Position">1</prop>
   </properties>
   <events>
    <script name="Changed" isEscaped="1"><![CDATA[main()
{
  int iDriverNumber;
  dyn_int diDriverNumbers;
  dyn_string dsServers, exceptionInfo;


  iDriverNumber = Treiber.value();
  getAllDrivers(diDriverNumbers, exceptionInfo);
  if( dynContains(diDriverNumbers, iDriverNumber) == 0 )
  {
    cmbEquipment.deleteAllItems();
    cmbEquipment.enabled(FALSE);

    cmbSubscription.deleteAllItems();
    cmbSubscription.enabled(FALSE);
  }
  else
  {
    if( returnServersInDriver(iDriverNumber, dsServers, exceptionInfo) == FALSE )
    {
      fwExceptionHandling_display(exceptionInfo);
    }

    if( dynlen(dsServers) &gt; 0 )
    {
      cmbEquipment.enabled(TRUE);
      cmbEquipment.items(dsServers);

      cmbSubscription.enabled(TRUE);
      fwPeriphAddressOPCUA_panel_updatePanelFillSubscriptions($sDpe, cmbEquipment.text());
    }
    else
    {
      cmbEquipment.deleteAllItems();
      cmbEquipment.enabled(FALSE);

      cmbSubscription.deleteAllItems();
      cmbSubscription.enabled(FALSE);
    }
  }

}





void getAllDrivers(dyn_int &amp;diDriverNumbers)
{
  int iLoop, iLen;
  dyn_string dsDps;


  dsDps = dpNames(&quot;_OPCUA*&quot;, &quot;_OPCUA&quot;);
  iLen  = dynlen(dsDps);
  for( iLoop = 1 ; iLoop &lt;= iLen ; iLoop++ )
  {
    dsDps[iLoop] = dpSubStr(dsDps[iLoop], DPSUB_DP);
    strreplace(dsDps[iLoop], &quot;_OPCUA&quot;, &quot;&quot;);
    diDriverNumbers[iLoop] = (int) dsDps[iLoop];
  }

}




bool returnServersInDriver(int iDriverNumber, dyn_string &amp;dsServers, dyn_string &amp;exceptionInfo)
{
  string sServerDp, sRunTimeServer;


  sServerDp = &quot;_OPCUA&quot; + iDriverNumber;
  if( !dpExists(sServerDp) )
  {
    fwException_raise(exceptionInfo, &quot;ERROR&quot;, &quot;Error, internal DP doesn't exist: &quot; + sServerDp, &quot;&quot;);
    return FALSE;
  }

  dpGet(sServerDp + &quot;.Config.Servers&quot;,    dsServers,
        sServerDp + &quot;.Command.AddServer&quot;, sRunTimeServer);

  if( sRunTimeServer != &quot;&quot; )
    dynAppend(dsServers, sRunTimeServer);

  dynUnique(dsServers);

  return TRUE;
}
]]></script>
