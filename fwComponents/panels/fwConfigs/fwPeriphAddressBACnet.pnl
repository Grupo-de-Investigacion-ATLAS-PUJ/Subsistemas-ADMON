/**
 * This function saves the BACnet settings in &quot;_address&quot; config of DP
 * 
 * @param exceptionInfo  Details of any errors are returned here
*/
fwPeriphAddressBACnet_saveSettings(dyn_string &amp;exceptionInfo)
{
  
  dyn_anytype addressConfig;
	
	addressConfig[fwPeriphAddress_TYPE] = fwPeriphAddress_TYPE_BACNET;	
  
  addressConfig[fwPeriphAddress_REFERENCE] = referenceField_BACnet.text;
  addressConfig[fwPeriphAddress_DRIVER_NUMBER] = (int)driverNumberSelector_BACnet.text;	
	
	addressConfig[fwPeriphAddress_ACTIVE] = activeCheckButton_BACnet.state(0);		
	addressConfig[FW_PARAMETER_FIELD_LOWLEVEL] = lowLevel_BACnet.state(0);
	
	addressConfig[FW_PARAMETER_FIELD_DATATYPE] = fwPeriphAddress_BACNET_TYPES[transformation_BACnet.selectedPos];
	
  addressConfig[fwPeriphAddress_DIRECTION] = fwPeriphAddressBACnet_getDirection(direction_BACnet.number, mode_BACnet.number);
  
  addressConfig[fwPeriphAddress_BACNET_POLL_GROUP] = &quot;&quot;;
  if(addressConfig[fwPeriphAddress_DIRECTION] == DPATTR_ADDR_MODE_IO_POLL ||
     addressConfig[fwPeriphAddress_DIRECTION] == DPATTR_ADDR_MODE_INPUT_POLL)
  {
    addressConfig[fwPeriphAddress_BACNET_POLL_GROUP] = &quot;_&quot; + pollGroupName_BACnet.selectedText;
  }
 
	fwPeriphAddress_set($sDpe, addressConfig, exceptionInfo, TRUE);
}

/** 
 * This function returns choosen I/O mode.
 *
 * @param directionMode direction mode (Output, Input, Input/Output)
 * @param receiveMode	receive mode (Unsolicited, Polling, Single Query, Alarm)
 * @return _address.._direction value for given directionMode and receiveMode
*/
int fwPeriphAddressBACnet_getDirection(int directionMode, int receiveMode)
{
  int addressMode = DPATTR_ADDR_MODE_UNDEFINED;
  switch(directionMode)
  {
    case fwPeriphAddress_BACNET_DIRECTION_OUT:
      addressMode = DPATTR_ADDR_MODE_OUTPUT;
      break;
    case fwPeriphAddress_BACNET_DIRECTION_IN:
      switch(receiveMode)
      {
        case fwPeriphAddress_BACNET_RECEIVE_UNSOLICITED:
          addressMode = DPATTR_ADDR_MODE_INPUT_SPONT;
          break;
        case fwPeriphAddress_BACNET_RECEIVE_SINGLE:
          addressMode = DPATTR_ADDR_MODE_INPUT_SQUERY;
          break;
        case fwPeriphAddress_BACNET_RECEIVE_POOLING:
          addressMode = DPATTR_ADDR_MODE_INPUT_POLL;
          break;
        case fwPeriphAddress_BACNET_RECEIVE_ALARM:
          break;
      }
      break;
    case fwPeriphAddress_BACNET_DIRECTION_INOUT:
      switch(receiveMode)
      {
        case fwPeriphAddress_BACNET_RECEIVE_UNSOLICITED:
          addressMode = DPATTR_ADDR_MODE_IO_SPONT;
          break;
        case fwPeriphAddress_BACNET_RECEIVE_SINGLE:
          addressMode = DPATTR_ADDR_MODE_IO_SQUERY;
          break;
        case fwPeriphAddress_BACNET_RECEIVE_POOLING:
          addressMode = DPATTR_ADDR_MODE_IO_POLL;
          break;
        case fwPeriphAddress_BACNET_RECEIVE_ALARM:
          break;
      }
      break;
  }
  return addressMode;
}

/**
 * This function create reference from the selected options and show it in referenceField_BACnet text field.
*/
string fwPeriphAddressBACnet_implodeReference()
{
  string referenceText;
  referenceText = device_BACnet.selectedText + fwPeriphAddress_BACNET_REF_SEPARATOR +
                               objectType_BACnet.selectedText + fwPeriphAddress_BACNET_REF_SEPARATOR +
                               objectIdSelector_BACnet.text + fwPeriphAddress_BACNET_REF_SEPARATOR +
                               property_BACnet.selectedText;
  if(indexField_BACnet.text != &quot;&quot;)
  {
    referenceText += fwPeriphAddress_BACNET_REF_SEPARATOR + indexField_BACnet.text;
  }
  referenceField_BACnet.text = referenceText;
}

]]></script>
