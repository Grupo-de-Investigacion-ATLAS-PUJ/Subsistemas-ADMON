  <shape Name="toggleControlBar" shapeType="PUSH_BUTTON" layerId="0">
   <properties>
    <prop name="serialId">85</prop>
    <prop name="Type"></prop>
    <prop name="RefPoint">998 9</prop>
    <prop name="Enable">True</prop>
    <prop name="Visible">True</prop>
    <prop name="ForeColor">_ButtonText</prop>
