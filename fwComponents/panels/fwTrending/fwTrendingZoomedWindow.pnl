    <prop name="HoverForeCol">blue</prop>
    <prop name="TabOrder">6</prop>
    <prop name="ToolTipText">
     <prop name="en_US.utf8">Toggle Menu Bar</prop>
    </prop>
    <prop name="maximumSize">30 12</prop>
    <prop name="layoutAlignment">AlignTop</prop>
    <prop name="snapMode">Point</prop>
    <prop name="Font">
     <prop name="en_US.utf8">Arial,-1,13,5,75,0,0,0,0,0</prop>
    </prop>
    <prop name="Location">998 9</prop>
    <prop name="Size">30 12</prop>
    <prop name="BorderStyle">None</prop>
    <prop name="Text">
     <prop name="en_US.utf8"></prop>
    </prop>
   </properties>
   <events>
    <script name="Clicked" isEscaped="1"><![CDATA[main()
{
  bool bControlBarOn;
  bit32 b32ControlBarBits;
  int iControlBar;
  string sRef, sTrendRunning, sButtonIcon;
  dyn_string dsPlotShapes, dsPlotData, exceptionInfo;


  sRef = $sRefName;


  // FWTREND-1097 - Need to update plotData for consistency with the plot configuration panel
  fwTrending_getRuntimePlotDataWithStrings(sRef, sTrendRunning, dsPlotShapes, dsPlotData, exceptionInfo, FALSE);

  iControlBar       = dsPlotData[fwTrending_PLOT_OBJECT_CONTROL_BAR_ON];
  b32ControlBarBits = iControlBar;
  bControlBarOn     = getBit(b32ControlBarBits,0);
  setBit(b32ControlBarBits, 0, !bControlBarOn);
  iControlBar       = b32ControlBarBits;
  dsPlotData[fwTrending_PLOT_OBJECT_CONTROL_BAR_ON] = iControlBar;
  // Changing alignment due to CloseButton
  fwTrending_setRuntimePlotDataWithStrings(sRef, sTrendRunning, dsPlotData, exceptionInfo, FALSE);

  //FWTREND-1097 Changing alignment due to CloseButton
  if (bControlBarOn )
  {
    sButtonIcon = &quot;[pattern,[fit,png,collapse_icon.png]]&quot;;
    this.layoutAlignment(&quot;AlignTop&quot;);
    LAYOUT_GROUP2.visible(1);
  }
  else
  {
    sButtonIcon = &quot;[pattern,[fit,png,expand_icon.png]]&quot;;
    LAYOUT_GROUP2.visible(0);
    this.layoutAlignment(&quot;AlignRight&quot;);
    this.visible(1);
  }
  setValue(sRef + &quot;toggleControlBar&quot;, &quot;fill&quot;, sButtonIcon);
}


]]></script>
    <script name="Initialize" isEscaped="1"><![CDATA[main()
{
}

]]></script>
