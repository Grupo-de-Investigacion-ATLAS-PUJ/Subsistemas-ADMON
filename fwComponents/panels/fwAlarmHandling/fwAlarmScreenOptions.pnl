  <script name="ScopeLib" isEscaped="1"><![CDATA[#uses &quot;fwAlarmHandling/fwAlarmHandling.ctl&quot;
#uses &quot;fwAlarmHandling/fwAlarmHandlingScreen.ctl&quot;
#uses &quot;fwAlarmHandling/fwAlarmHandlingScreenGroups.ctl&quot;
#uses &quot;fwAlarmHandling/fwAlarmScreen.ctl&quot;
#uses &quot;fwAlarmHandling/fwAlarmScreenGeneric.ctl&quot;

unsigned g_descriptionOption;
bool g_rowColourOption;
int g_scrollLockTimeout;
bool g_showOnlineValue;
bool g_colourWholeRow;
bool g_bGroupsEnabled;
bool g_bAlarmFilterEnabled;

bool g_bInitialized = false;


showRestartWarning()
{
  if
    (
      (colourWholeRow.state(0) == g_rowColourOption) &amp;&amp; 
      ((showDpeInDescription.number == g_descriptionOption) || !showDpeInDescription.enabled) &amp;&amp; 
      ((int) (scrollLock.state(0)?scrollLockValue.text:0) == g_scrollLockTimeout) &amp;&amp;
      (columnValueOptions.state(1) == g_showOnlineValue) &amp;&amp;
      (checkBoxShowGroups.state(0) == g_bGroupsEnabled) &amp;&amp; (checkBoxShowAlarmFilter.state(0) == g_bAlarmFilterEnabled)
    )
  {
    restartNote.visible = FALSE;
    infoArrow.visible = FALSE;
  }
  else
  {
    restartNote.visible = TRUE;
    infoArrow.visible = TRUE;
  }
}


/*
  Check if enable groups and enable alarm filter are not
  both disabled
  */
checkIfOneOptionEnabled( string sOptionWidgets)
{
    bool bEnableGroups = checkBoxShowGroups.state(0);
    bool bEnableAlarmFilter = checkBoxShowAlarmFilter.state(0);
    
    if ( !bEnableGroups &amp;&amp; !bEnableAlarmFilter)
    {
      int x, y;
      getValue( &quot;checkBoxShowAlarmFilter&quot;, &quot;position&quot;, x, y);
      enableOptionsText.position(x+100, y-15);
      enableOptionsText.visible(true);
      setValue( sOptionWidgets, &quot;state&quot;, 0, true);
      fwAlarmScreen_saveConfig();
    } else
    {
      enableOptionsText.visible(false);
    }
}]]></script>
  <script name="Close" isEscaped="1"><![CDATA[main()
{
	return;
}]]></script>
  <script name="Initialize" isEscaped="1"><![CDATA[main()
{
  dyn_string dsExceptions;
  string sDefaultFilter;
  dyn_string dsConfigDpNames;

  fwAlarmScreen_getDescriptionColumnBehaviour(g_descriptionOption, dsExceptions);
  showDpeInDescription.number = g_descriptionOption;

  fwAlarmScreen_getRowColourBehaviour(g_rowColourOption, dsExceptions);
  colourWholeRow.state(0) = g_rowColourOption;
  
  
  fwAlarmScreen_getScrollLockTimeout(g_scrollLockTimeout, dsExceptions);
  scrollLock.state(0) = (g_scrollLockTimeout != 0);
  scrollLockValue.enabled = (g_scrollLockTimeout != 0);
  scrollLockValue.text = g_scrollLockTimeout == 0 ? 10 : g_scrollLockTimeout;        
	
  bool bColumnVisible;
  fwAlarmScreen_getShowHideColumn(fwAlarmScreen_PROPERTIES_DP, fwAlarmScreen_COLUMN_DP_NAME, bColumnVisible, dsExceptions);  
  columnVisibleOptions.state(0) = bColumnVisible;  
  
  fwAlarmScreen_getShowHideColumn(fwAlarmScreen_PROPERTIES_DP, fwAlarmScreen_COLUMN_LOGICAL_NAME, bColumnVisible, dsExceptions);  
  columnVisibleOptions.state(1) = bColumnVisible;  
  
  fwAlarmScreen_getShowHideColumn(fwAlarmScreen_PROPERTIES_DP, fwAlarmScreen_COLUMN_DESCRIPTION, bColumnVisible, dsExceptions);  
  columnVisibleOptions.state(2) = bColumnVisible;
		
  fwAlarmScreen_getShowHideColumn(fwAlarmScreen_PROPERTIES_DP, fwAlarmScreen_COLUMN_ALERT_VALUE, bColumnVisible, dsExceptions);  
  columnValueOptions.state(0) = bColumnVisible;
  
  fwAlarmScreen_getShowHideColumn(fwAlarmScreen_PROPERTIES_DP, fwAlarmScreen_COLUMN_ONLINE_VALUE, bColumnVisible, dsExceptions);  
  columnValueOptions.state(1) = bColumnVisible;
  g_showOnlineValue = bColumnVisible;

  fwAlarmScreen_getShowHideColumn(fwAlarmScreen_PROPERTIES_DP, fwAlarmScreen_COLUMN_TIME_CAME, bColumnVisible, dsExceptions);  
  cameWentTimeSelection.selectedPos = bColumnVisible? 2 : 1;
  
  int iIdleTimeout;
  fwAlarmScreen_getIdleTimeout(iIdleTimeout, dsExceptions);
  idleTimeoutValue.text = iIdleTimeout;
  idleTimeout.state(0) = iIdleTimeout;
  idleTimeoutValue.enabled = iIdleTimeout;
                           
  showDpeInDescription.enabled = columnVisibleOptions.state(2);
        
  int iOnlineValueUpdateRate;
  fwAlarmScreen_getOnlineValueUpdateRate(iOnlineValueUpdateRate, dsExceptions);
  onlineValueUpdateRate.text = iOnlineValueUpdateRate;
  onlineValueUpdateRate.enabled = columnValueOptions.state(1);
        
  bool bShowDistDetails;
  fwAlarmScreen_getDistSystemDisplayOption(bShowDistDetails, dsExceptions);
  showDistSystemDetails.state(0) = bShowDistDetails;

  int iSummaryMode;
  fwAlarmScreen_getReductionMode(fwAlarmScreen_PROPERTIES_DP, iSummaryMode, dsExceptions);
  switch(iSummaryMode)
  {
    case AES_SUMALERTS_BOTH:
      summariesMode.number = 0;
      break;
    case AES_SUMALERTS_FILTERED:
      summariesMode.number = 1;
      break;
    case AES_SUMALERTS_NO:
      summariesMode.number = 2;
      break;
  }
  
  fwAlarmScreen_getDefaultFilterName(sDefaultFilter, dsExceptions);
  strreplace(sDefaultFilter, fwAlarmScreen_FILTER_DP_PREFIX, &quot;&quot;);
	dsConfigDpNames = dpNames(fwAlarmScreen_FILTER_DP_PREFIX + &quot;*&quot;, fwAlarmScreen_FILTER_DP_TYPE);
	
	for(int i = 1; i &lt;= dynlen(dsConfigDpNames) ; i++)
	{
		dsConfigDpNames[i] = dpSubStr(dsConfigDpNames[i], DPSUB_DP);
		strreplace(dsConfigDpNames[i], fwAlarmScreen_FILTER_DP_PREFIX, &quot;&quot;);
	}
  
  dynInsertAt(dsConfigDpNames, &quot;Show All (*)&quot;, 1);
  savedFilterList.items(dsConfigDpNames);
  
  if(dynContains(dsConfigDpNames, sDefaultFilter) &gt; 0)
  {
    savedFilterList.text = sDefaultFilter;
  }
  else
  {
    savedFilterList.selectedPos = 1;
  }
  
  int iMaxLines;
  fwAlarmScreen_getHistoricalMaxLines(iMaxLines,dsExceptions);
  historicalMaxLineslValue.text = iMaxLines;
  
  g_bGroupsEnabled = fwAlarmScreen_getEnableGroups();
  checkBoxShowGroups.state(0, g_bGroupsEnabled);
  
  g_bAlarmFilterEnabled = fwAlarmScreen_getEnableAlarmFilter();
  checkBoxShowAlarmFilter.state(0, g_bAlarmFilterEnabled);
  
  checkboxInvertPriorities.state(0, fwAlarmScreen_getInvertPriorities());
  
  g_bInitialized = true;
}
]]></script>
