{
	int i, rows;
	dyn_float df;
	dyn_string ds, systems;
	

  rows = this.lineCount();
  for(i=0; i&lt;rows; i++)
  {
    systems[i+1] = this.cellValueRC(i, &quot;systemName&quot;);
  }

  while(dynContains(systems, &quot;&quot;) &gt; 0)
  {
    dynRemove(systems, dynContains(systems, &quot;&quot;));
  }

  ChildPanelOnCentralModalReturn(
      &quot;fwAlarmHandling/fwAlarmScreenFilterSystemPicker.pnl&quot;,
      &quot;System Picker&quot;, 
      makeDynString(&quot;$dsSelectedSystems:&quot; + systems),
      df, 
      ds
    );

  if(df[1] &gt; 0.5)
  {
    this.deleteAllLines();
    
    this.appendLines(dynlen(ds), &quot;systemName&quot;, ds);
  }
  
  this.lineVisible = 0;
}
]]></script>
