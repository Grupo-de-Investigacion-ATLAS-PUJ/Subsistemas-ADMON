  // Apply it  
  string sAesProperties = aes_getPropDpName(AES_DPTYPE_PROPERTIES, true, AESTAB_TOP, false, false);   
  fwAlarmScreen_applyFilter(sAesProperties, ddaAesFilter, dsExceptions);
