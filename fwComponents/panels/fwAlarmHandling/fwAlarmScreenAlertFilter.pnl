  dyn_string dsExceptions;
  
  
  // Load a quick filter : display it as current filter.
  string sQuickFilter = this.text();
  
  dyn_dyn_anytype ddaAesFilter;
  
  if (&quot;&quot; == sQuickFilter) // Default filter
  {    
	  _fwAlarmScreen_getDefaultFilter(ddaAesFilter, dsExceptions);
  }
  else
  {    
    fwAlarmScreen_loadFilter(fwAlarmScreen_FILTER_DP_PREFIX + sQuickFilter, ddaAesFilter, dsExceptions);    
  }
  
  if (dynlen(dsExceptions) &gt; 0)
  {
